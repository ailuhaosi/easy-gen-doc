{
    "compilerOptions": {
        "target": "esnext",
        "useDefineForClassFields": true,
        "module": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "jsx": "preserve",
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "allowJs": true,
        "lib": [
            "esnext",
            "dom"
        ]
    },
    "include": [
        "gd-accbuild-doc/.vuepress/**/*.ts",
        "gd-accbuild-doc/.vuepress/**/*.d.ts",
        "gd-accbuild-doc/.vuepress/**/*.tsx",
        "gd-accbuild-doc/.vuepress/**/*.vue"
    ],
}